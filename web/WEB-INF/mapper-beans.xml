<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="spring-dao.xml" />

    <bean id="courseMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
        <property name="mapperInterface" value="net.yorkjr.jareau.mapper.CourseMapper" />
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />
    </bean>

    <bean id="companyAgentMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
        <property name="mapperInterface" value="net.yorkjr.jareau.mapper.CompanyAgentMapper" />
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />
    </bean>

    <bean id="questionDataAccessor" class="org.mybatis.spring.mapper.MapperFactoryBean">
        <property name="mapperInterface" value="net.yorkjr.jareau.exambank.dao.QuestionDataAccessor" />
        <property name="sqlSessionFactory" ref="sqlSessionFactory" />
    </bean>
</beans>
